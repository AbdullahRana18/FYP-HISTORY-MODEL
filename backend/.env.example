# Copy this file to .env and fill in your actual API keys
# DO NOT commit the .env file to GitHub

# Required: Groq API Key (Primary LLM Engine)
# Get your key from: https://console.groq.com
GROQ_API_KEY=your_groq_api_key_here

# Optional: Hugging Face API Key (Fallback LLM Engine)
# Get your key from: https://huggingface.co/settings/tokens
HF_API_KEY=your_hf_api_key_here

# Optional: MongoDB connection (not required for basic usage)
MONGO_URI=mongodb://127.0.0.1:27017

# Optional: Secret key for JWT (not required for basic usage)
SECRET_KEY=your_secret_key_here
